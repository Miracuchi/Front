{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/require-default-props": ["off"],
        "react/forbid-prop-types": ["off"],
        "react/no-unknown-property": ["off"],
        "jsx-a11y/click-events-have-key-events" : ["off"]
    }
}
